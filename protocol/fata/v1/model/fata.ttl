<Note>
    a                owl:Class ;
    rdfs:subClassOf  prov:Entity ;
    rdfs:label       "Note"@en ;
    rdfs:label       "Notiz"@de ;
    skos:notation    "fata:Note"^^xsd:string ;
    skos:prefLabel   "Note"^^xsd:string ;
    rdfs:isDefinedBy fata: .

<subject>
    a                owl:ObjectProperty ;
    rdfs:label       "Subject"@en ;
    rdfs:label       "Subjekt"@de ;
    skos:notation    "fata:subject"^^xsd:string ;
    skos:prefLabel   "Subject"^^xsd:string ;
    rdfs:domain      :Note ;
    rdfs:range       xsd:string ;
    rdfs:isDefinedBy fata: .

<message>
    a                owl:ObjectProperty ;
    rdfs:label       "Message"@en ;
    rdfs:label       "Nachricht"@de ;
    skos:notation    "fata:message"^^xsd:string ;
    skos:prefLabel   "Message"^^xsd:string ;
    rdfs:domain      :Note ;
    rdfs:range       xsd:string ;
    rdfs:isDefinedBy fata: .

<note>
    a                owl:ObjectProperty ;
    rdfs:label       "Note"@en ;
    rdfs:label       "Notiz"@de ;
    skos:notation    "fata:note"^^xsd:string ;
    skos:prefLabel   "Notiz"^^xsd:string ;
    rdfs:domain      :Envelope ;
    rdfs:range       Note: ;
    rdfs:isDefinedBy fata: .
